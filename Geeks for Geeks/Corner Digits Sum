import java.util.Scanner;
class Main{
    public static void main(String[] args){
        int n=12;
        int fd=0;
        int last=n%10;
        if(n<=9)
            {
                last=0;
            }
        while(n>0)
        {
            fd=n;
            n=n/10;
        }
        int sum=fd+last;
        System.out.println(sum);
    }
}
