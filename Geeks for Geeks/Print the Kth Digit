import java.util.Scanner;
class Main{
    public static void main(String[] args){
        int a=3;
        int b=5;
        int k=3;
        long ld=0;
        long power=(long)Math.pow(a,b);
        System.out.println(power);
        int i=1;
        while(i<=k)
        {
             ld=power%10;
            power=power/10;
              i++;
        }
        System.out.println(ld);
    }
}
