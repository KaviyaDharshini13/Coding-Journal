class Main {
    public static void main(String[] args) {
        long n=5;
        long sum=0;
        long b=2;
        long rum=0;
        for(int i=1;i<=n;i++)
        {
            long a=(long)Math.pow(i,b);
            sum=sum+a;
            rum=rum+i;
        }
        long sub=(long)Math.pow(rum,b);
        long diff=sub-sum;
     System.out.println(diff);
    }
}
