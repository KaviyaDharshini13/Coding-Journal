import java.io.*;
import java.util.*
public class Solution {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int sum=0;
        int x=n;
        while(n>0)
        {
            int last=n%10;
            sum=(sum*10)+last;
            n=n/10;
        }
        if(sum==x)
        {
         System.out.println("true");
        }
        else
        {
            System.out.println("false");
        }
    }
}
