import java.util.Scanner;
class Main{
    public static void main(String[] args){
        int n=534564;
        int count=0;
        while(n>0)
        {
            int ld=n%10;
            n=n/10;
            if(ld%2!=0)
            {
                count=count+1;
            }
        }
        System.out.println(count);
    }
}
